{% extends 'base.html.twig' %}

{% block content %}
<table>
    {% for sign in signs  %}
    <tr>
        <td>
                {{ sign }}
        </td>
        <td>
            <a href="{{ path('rewrite-horoscope', {'year': date|date("Y"), 'month': date|date("m"), 'day': date|date("d"), 'sign': sign}) }}" class="font-weight-bold text-dark">
                {{ path('rewrite-horoscope', {'year': date|date("Y"), 'month': date|date("m"), 'day': date|date("d"), 'sign': sign}) }}
            </a>
        </td>
    </tr>
    {% endfor %}
</table>

{% endblock %}
